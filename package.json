{
  "name": "moltin-shopkit-nano",
  "version": "0.0.1",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv --host=0.0.0.0 --port=8000 dist",
    "start:dev": "sirv --dev --host=0.0.0.0 --port=8000 dist",
    "compress": "run-p compress:gzip compress:brotli",
    "compress:gzip": "gzip --best --keep --force -- dist/app.js",
    "compress:brotli": "brotli --best --keep --force -- dist/app.js"
  },
  "author": "s0kil",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.15.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-minifyliterals": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.9.0",
    "rollup-plugin-sizes": "^0.5.1",
    "rollup-plugin-terser": "^5.0.0",
    "sirv-cli": "^0.4.4"
  },
  "dependencies": {
    "fastdom": "^1.0.9",
    "stage0": "0.0.24"
  }
}
